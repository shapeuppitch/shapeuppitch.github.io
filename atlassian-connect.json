{
    "key": "io.curiouslab.confluence.shapeuppitch",
    "name": "Shape Up Pitch Template",
    "description": "Provides template for writing up shaped work as a pitch. This is for teams who want to practise the Shape Up approach.",
    "vendor": {
        "name": "CuriousLab",
        "url": "https://www.curiouslab.io/"
    },
    "baseUrl": "https://shapeuppitch.curiouslab.io/",
    "authentication": {
        "type": "jwt"
    },
    "lifecycle": {
        "installed": "/installed"
    },
    "scopes": [
        "READ"
    ],
    "apiMigrations": {
        "signed-install": true
    },
    "modules": {
        "blueprints": [
            {
                "template": {
                    "url": "/blueprint.xml"
                },
                "createResult": "edit",
                "key": "shape-up-pitch",
                "name": {
                    "value": "Shape Up Pitch"
                },
                "icon": {
                    "width": 144,
                    "height": 144,
                    "url": "blueprint-icon.png"
                }
            }
        ]
    }
}
